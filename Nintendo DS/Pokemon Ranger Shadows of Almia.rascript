//Gathers suof all bitflags for Browser registering. (Seen & Captured)
function browser_loop(memType){
    trigger = 0    
    if(memType == "delta"){
        for dex_entry in pokemon_database{        
            trigger = trigger + prev(pokemon_database[dex_entry]["dex_two"])            
          }           
          return trigger < 269         
        }
        else{     
        for dex_entry in pokemon_database{
            trigger = trigger + pokemon_database[dex_entry]["dex_two"]                         
          }    
          return measured(trigger == 269)                           
       }                      
}
