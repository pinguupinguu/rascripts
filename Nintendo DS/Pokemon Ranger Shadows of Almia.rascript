//Loops through browser rank data. Repurposed as of 7/2/2021 - Tkt ID: 39878
function capture_loop(mem_type){            
       trigger = []              
       for rank_entry in pokemon_database{
           if(mem_type == "isDelta")          
                array_push(trigger, prev(pokemon_database[rank_entry]["cptr_one"]) == 0 && once(prev(pokemon_database[rank_entry]["cptr_two"]) == 0))           
           else
                array_push(trigger, pokemon_database[rank_entry]["cptr_one"] == 0 && once(pokemon_database[rank_entry]["cptr_two"] == 0))
       }
       return trigger        
    }
