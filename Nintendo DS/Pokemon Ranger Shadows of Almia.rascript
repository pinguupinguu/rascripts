//=====ROOM CHALLENGES=====
// $0bbae0: [16bit] On-Screen Timer for Events (Frames)
//          Used at Team Dim Sun Base
function frame_timer() => word(0x0bbae0)
// $0B333C: [16bit] Player x
//          For Volcano Cave:
//          Left Extremity X Coord = 0xd4
//          Right Extremity X Coord = 0x190
function player_x_coord() => word(0x0b333c)
// $0B333E: [16bit] Player Y
//          For Volcano Cave:
//          Y-Coordinate Check = 0x127
function player_y_coord() => word(0x0b333e)
achievement(
    "[m] Dim Sun Speed Run",
    "Reach Team Dim Sun's Cargo Ship in Volcano Cave within 10 seconds or less",
    id=159568,
    badge="179430",
    points=5,
    trigger=
        unless(repeated(0x258, frame_timer() < prev(frame_timer()))) && //Time in Frames: 0x258 = 10 seconds.
        location() == 0x8d && //Volcano Cave - Team Dim Sun Ship        
        player_x_coord() >= 0xd4 && 
        player_x_coord() <= 0x190 &&
        trigger_when(player_y_coord() <= 0x127) &&        
        (always_false() || never(frame_timer() > prev(frame_timer()))) //Reset if timer is reset to 2/3 minutes.
)
achievement(
    "Qwilfish Fun!",
    "Swim through the Qwilfish room in the Sea of Wailord without taking styler damage",
    id=159569,
    badge="179429",
    points=5,
    trigger=            
        never(prev(location()) == 0x101 && location() == 0x102) && //Prevents player from cheesing by passing through tube to get around room.
        never(environment_health() < prev(environment_health())) &&
        once(prev(location()) == 0x101 && location() == 0x105) &&//Set hit from beginning of previous room.
        trigger_when(prev(location()) == 0x105 && location() == 0x102)
)
//=====BOSS S-RANK CHALLENGES=====
function in_swap() => byte(0x08af58) == 0x2
function boss_rank_capture(cptr_one, cptr_two, dex_two) =>
    save_check() &&
    prev(dex_two) == 0 &&
    prev(cptr_one) + prev(cptr_two) != 0 &&
    cptr_one +
    cptr_two == 0 &&
    in_swap()
